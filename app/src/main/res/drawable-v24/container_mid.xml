<?xml  version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="270"
                android:endColor="@color/neumorph_outline_end"
                android:startColor="@color/neumorph_outline_start"
                android:type="linear"/>

        </shape>
    </item>
    <item
        android:bottom="1.0dp"
        android:left="3.0dp"
        android:right="3.0dp"
        android:top="1.0dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="@color/colorBackground"
                android:startColor="@color/colorBackground"
                android:type="linear"/>

        </shape>
    </item>
</layer-list>